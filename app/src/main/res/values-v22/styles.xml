<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="textToolbarTitle">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#d3d3d3</item>
    </style>
    <style name="textToolbarContent">
        <item name="android:textColor">#70ffffff</item>
        <item name="android:textSize">12sp</item>
    </style>

    <color name="color_747474">#747474</color>
    <!--分割线-->
    <style name="Divider">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:background">#dcdcdc</item>
    </style>

    <style name="Divider.Line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="Divider.Line.Vertical">
        <item name="android:layout_height">1px</item>
    </style>
</resources>